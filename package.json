{
  "title": "element-animate-polyfill",
  "main": "dist/element-animate-polyfill.js",
  "dependencies": {
    "clone": "^1.0.2",
    "gulp": "^3.9.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
